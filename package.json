{
  "name": "arifmeticalino",
  "version": "1.0.0",
  "description": "Arithmetic operations trainer for kids",
  "main": "arifmeticalino.js",
  "scripts": {
    "build:1": "node node_modules/xfs/rmdir dist",
    "build:2": "node node_modules/xfs/copy ./src ./dist/build",
    "build:3": "tsc -p tsconfig.prod.json",
    "build:4": "webpack --config webpack.config.prod.js --progress --profile --bail",
    "build": "npm run build:1 && npm run build:2 && npm run build:3 && npm run build:4",
    "test": "karma start",
    "test:watch": "karma start --no-single-run --auto-watch"
  },
  "keywords": [
    "arithmetic",
    "trainer",
    "kids"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/OleksandrTheGreat/arifmeticalino.git"
  },
  "author": "OleksandrTheGreat",
  "license": "ISC",
  "dependencies": {
    "abus": "..//ABus//dist//bin"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.51",
    "awesome-typescript-loader": "^3.1.3",
    "jasmine": "^2.6.0",
    "karma": "^1.7.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.3",
    "requirejs": "^2.3.3",
    "sourcemap": "^0.1.0",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1",
    "xjasmine": "../xjasmine"
  }  
}
